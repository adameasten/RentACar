@model CarDetailsVM

<div class="container">
    <div class="row">
        <div class="col">
            <h2> @Model.car.Model</h2>
        </div>
    </div>
    <div class="row">
        <div class="col">
            <img src="@Model.car.ImgUrl">
        </div>
    </div>
    <div class="row">
        <div class="col">
            <p>@Model.car.Description</p>
        </div>
    </div>
    <div class="row">
        <div class="col">
            <p>Info</p>
            <ul>
                <li>Bränsletyp: @Model.car.Fuel</li>
                <li>Antal säten: @Model.car.Seats</li>
                <li>Antal dörrar: @Model.car.Doors</li>
                <li>Växellåda: @Model.car.Gear</li>
                <li>Kilometerantal: @Model.car.Km</li>
                <li>Biltyp: @Model.car.Type</li>
            </ul>
        </div>
    </div>

    <div class="row">
        <div class="col">
            <p>Tillbehör</p>
            <ul>
                <li>Husdjur: @Model.car.Pets</li>
                <li>Takräcke: @Model.car.RoofRack</li>
                <li>Dragkrok: @Model.car.TowBar</li>
                <li>Barnstol: @Model.car.ChildSeat</li>
                <li>Ac: @Model.car.Ac</li>
            </ul>
        </div>
    </div>
</div>


<partial name="_RentFormDetails" model="@Model.form" />
