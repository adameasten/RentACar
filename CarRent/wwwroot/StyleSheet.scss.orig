.button-default {
    font-size: 1em;
    padding: 0.5em 0.75em;
    display: inline-block;
    width: auto;
    height: auto;
    cursor: pointer;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    -o-border-radius: 5px;
    -ms-border-radius: 5px;
    -khtml-border-radius: 5px;
    border-radius: 5px;
    background-color: $greenlogocolor;
    background: -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, $greenlogocolor), color-stop(100%, #6fa88d));
    background: -webkit-linear-gradient($greenlogocolor, #6fa88d);
    background: -moz-linear-gradient($greenlogocolor, #6fa88d);
    background: -o-linear-gradient($greenlogocolor, #6fa88d);
    background: -ms-linear-gradient($greenlogocolor, #6fa88d);
    background: linear-gradient($greenlogocolor, #6fa88d);
    -moz-box-shadow: #c1e4d4 0 1px 0 inset;
    -webkit-box-shadow: #c1e4d4 0 1px 0 inset;
    -o-box-shadow: #c1e4d4 0 1px 0 inset;
    box-shadow: #c1e4d4 0 1px 0 inset;
    border: 1px solid #5f9e80;
    color: white;
    text-shadow: #40936b 0 -1px 0;
    text-decoration: none;

    &:link {
        text-decoration: none;
        color: white;
    }

    &:visited {
        text-decoration: none;
        color: white;
    }

    &:hover {
        text-decoration: none;
        color: white;
    }

    &:active {
        text-decoration: none;
        color: white;
    }
}

