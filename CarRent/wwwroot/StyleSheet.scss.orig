.zoom {
    transition: transform .2s; /* Animation */
}

.zoom:hover {
    cursor: pointer;
    transform: scale(1.05); /* (150% zoom - Note: if the zoom is too large, it will go outside of the viewport) */
}
.zoomMin {
    transition: transform .2s; /* Animation */
}

.zoomMin:hover {
    cursor: pointer;
    transform: scale(1.01); /* (150% zoom - Note: if the zoom is too large, it will go outside of the viewport) */
}


.section {
    position: relative;
    height: 70vh;
}

#overrideStartingHour select{
    padding: 2px;
}

#overrideEndingHour select{
    padding: 2px;
}

#booking .section-center .container .row{
    justify-content: center;
}

.section .section-center {
    position: absolute;
    top: 50%;
    left: 0;
    right: 0;
    -webkit-transform: translateY(-50%);
    transform: translateY(-50%);
    justify-content: center;
}

.booking-form {
    background: white;
    border-radius: 2px;
}

.booking-form > form .row.no-margin {
    margin-right: 0px;
    margin-left: 0px;
}

.booking-form > form .row.no-margin > [class*="col-"] {
    padding-right: 0px;
    padding-left: 0px;
}

.booking-form .form-group {
    position: relative;
    margin-bottom: 0px;
    padding: 18px;
}

.booking-form .form-control {
    background-color: transparent;
    border-radius: 0px;
    border: none;
    height: 40px;
    -webkit-box-shadow: none;
    box-shadow: none;
    font-size: 15px;
    color: #222;
    font-weight: 400;
    padding: 0;
}

.booking-form .form-group:after {
    content: '';
    background: rgba(129, 131, 144, 0.15);
    position: absolute;
    top: 25px;
    bottom: 0;
    right: 0px;
    width: 2px;
    height: 75px;
}

.booking-form .form-btn {
    padding: 25px;
}

.booking-form .submit-btn {
    background: $greenlogocolor;
    border: none;
    text-transform: capitalize;
    display: block;
    border-radius: 2px;
    width: 100%;
    height: 75px;
    font-size: 16px;
    font-weight: 400;
    color: #fff;
}

